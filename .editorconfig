# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Java editor styling
[*.java]
indent_style = tab
indent_size = 4

# XML editor styling
[*.xml]


# YAML editor styling 
[*.yml]
indent_style = space
indent_size = 2
